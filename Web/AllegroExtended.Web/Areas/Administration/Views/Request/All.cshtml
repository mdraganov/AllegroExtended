@model List<AllegroExtended.Web.Areas.Administration.ViewModels.AccountRequestListViewModel>
@{
    ViewBag.Title = "All Account Requests";
}

<h2 class="text-muted">@ViewBag.Title</h2>
@if (this.TempData["Notification"] != null)
{
    <div class="alert alert-warning alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert">x</button>
        @this.TempData["Notification"]
    </div>
}
<table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>#</th>
            <th>Email</th>
            <th>Department</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            @Html.DisplayFor(m => m[i], new { row = i + 1 });
        }
    </tbody>
</table>